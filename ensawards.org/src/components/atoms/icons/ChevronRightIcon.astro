---
import { cn } from "../../../utils/tailwindClassConcatenation";
import { IconProps } from "./types";

const { className }: IconProps = Astro.props;
---

<svg
  width="15"
  height="16"
  viewBox="0 0 15 16"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
  class={cn("text-#404040 opacity-40 transition-all duration-200", className)}>
  <g>
    <path
      d="M5.625 11.75L9.375 8L5.625 4.25"
      stroke="currentColor"
      stroke-width="1.33"
      stroke-linecap="round"
      stroke-linejoin="round"></path>
  </g>
</svg>
